pipeline {
    agent {
       label 'new-one'
   }

    
    tools {
        maven 'maven'
        jdk 'jdk 17'
    }
    
    stages {        
        stage('Compile') {
            steps {
                sh "mvn compile"
            }
        }
        
        stage('Test') {
            steps {
                sh "mvn test"
            }
        }
        
        stage('Build') {
            steps {
                sh "mvn package"
            }
        }
    }
}
